<Window x:Class="APM_of_accounting_of_academic_performance.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:APM_of_accounting_of_academic_performance"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1000"   MinHeight="700" MaxHeight="1080" MinWidth="1000" MaxWidth="1920">
    <Grid>
        <Grid.RowDefinitions >
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10" Grid.Row="0">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10" Grid.Row="0">
                <Button Style="{StaticResource SmallButton}" Click="BackButton_Click" Name="BackButton" >Назад</Button>
            </StackPanel>
        </StackPanel>

        <Frame x:Name="MainPageFrame" Grid.Row="1" NavigationUIVisibility="Hidden" Navigated="MainPageFrame_Navigated"></Frame>

    </Grid>
</Window>
